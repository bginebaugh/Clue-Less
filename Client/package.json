{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=DEV concurrently \"babel-node server.js\" \"sleep 2 && electron .\" --inline --content-base . --history-api-fallback -devtool -d",
